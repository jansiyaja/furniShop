<script>
  function showPage(pageNumber) {
      const tableRows = document.querySelectorAll('#ordersTable tbody');
      tableRows.forEach((row, index) => {
          if (index + 1 <= pageNumber * 5 && index + 1 > (pageNumber - 1) * 5) {
              row.classList.remove('hidden');
          } else {
              row.classList.add('hidden');
          }
      });
  }
</script>
