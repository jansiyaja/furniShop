    <script>
        function IncDecValue(productId, count, index) {
            console.log('Calling IncDecValue with index:', index);
            const data = { productId, count, };

            $.ajax({
    url: '/updateQuantity',
    method: 'POST',
    data: JSON.stringify(data),
    contentType: 'application/json',
    success: function (response) {
        if (response.success) {
            updateUI(index, response.newQuantity, response.newTotalPrice);
            location.reload();
        } else if (response.min) {
            Swal.fire({
                icon: "warning",
                title: "Oops...",
                text: "Minimum stock reached",
            });
        } else if (response.max) {  // Corrected condition
            Swal.fire({
                icon: "warning",
                title: "Oops...",
                text: "Maximum stock reached",
            });
        }
    }
});

        }

        function updateUI(index, newQuantity, newTotalPrice) {
            console.log(`updateUI called with index: ${index}, newQuantity: ${newQuantity}, newTotalPrice: ${newTotalPrice}`);

            // Update the DOM elements
            $(`#quantityInput-${index}`).val(newQuantity);
            $(`.total-col-${index}`).text(`$${newTotalPrice.toFixed(2)}`);
        }


        function deleteCart(proId) {
    Swal.fire({
        title: 'Remove product from cart?',
        text: 'This action cannot be undone.',
        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#3085d6',
        cancelButtonColor: '#d33',
        confirmButtonText: 'Yes, remove it!'
    }).then((result) => {
        if (result.isConfirmed) {
            $.ajax({
                url: '/removeCart',
                data: {
                    product: proId
                },
                method: 'POST',
                success: function (response) {
                    if (response.success == true) {
                        Swal.fire({
                            icon: 'success',
                            title: 'Item Removed',
                            text: 'Your item has been successfully removed from the cart.',
                        }).then(() => {
                            location.reload();
                        });
                    }
                }
            });
        }
    });
}


