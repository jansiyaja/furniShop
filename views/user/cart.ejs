<%- include ('../Layout/user/header.ejs') -%>
    <div class="page-wrapper">
        <%- include ('../Layout/user/navbar.ejs') -%>

        <main class="main">
            <div class="page-header text-center" style="background-image: url('assets/images/page-header-bg.jpg')">
                <div class="container">
                    <h1 class="page-title">Shopping Cart<span>Shop</span></h1>
                </div><!-- End .container -->
            </div><!-- End .page-header -->
            <nav aria-label="breadcrumb" class="breadcrumb-nav">
                <div class="container">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                        <li class="breadcrumb-item"><a href="#">Shop</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Shopping Cart</li>
                    </ol>
                </div><!-- End .container -->
            </nav><!-- End .breadcrumb-nav -->

            <div class="page-content">
                <div class="cart">
                    <% if (cartDetails && cartDetails.products && cartDetails.products.length> 0) { %>
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-9">
                                    <table class="table table-cart table-mobile">
                                        <thead>
                                            <tr>
                                                <th>Product</th>
                                                <th>Price</th>
                                                <th>Quantity</th>
                                                <th>Total</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% cartDetails.products.forEach((value, index)=> { %>
                                                <tr>
                                                    <td class="product-col">
                                                        <div class="product">
                                                            <figure class="product-media">
                                                                <a href="#">
                                                                    <% if (value.productId && value.productId.images &&
                                                                        value.productId.images[0]) { %>
                                                                        <img src="<%= value.productId.images[0] %>"
                                                                            alt="Product image">
                                                                        <% } else { %>
                                                                            <img src="/path/to/default/image.jpg"
                                                                                alt="Default image">
                                                                            <% } %>
                                                                </a>
                                                            </figure>
                                                            <h3 class="product-title">
                                                                <a href="#">
                                                                    <%= value.productId ? value.productId.name
                                                                        : 'Product Name N/A' %>
                                                                </a>
                                                            </h3><!-- End .product-title -->
                                                        </div><!-- End .product -->
                                                    </td>


                                                    <td class="price-col">$<%= value.productId.price %>
                                                    </td>
                                                    <td class="quantity-col ">
                                                        <div class="cart-product-quantity">
                                                            <div class="quantity-control d-flex">
                                                                <button type="button"
                                                                    onclick="IncDecValue('<%= value.productId._id %>', -1)"
                                                                    style="border-color: #a6c76c;">-</button>
                                                                <input type="string" id="quantityInput-<%= index %>"
                                                                    class="form-control" value="<%= value.quantity %>"
                                                                    min="1" max="5" step="1" data-decimals="0"
                                                                    style="width: 70px; text-align: center;">
                                                                <button type="button"
                                                                    onclick="IncDecValue('<%= value.productId._id %>', 1)"
                                                                    style="border-color: #a6c76c;">+</button>
                                                            </div>
                                                        </div><!-- End .cart-product-quantity -->

                                                    </td>
                                                    <td class="total-col">$<%= (value.quantity *
                                                            value.productId.price).toFixed(2) %>
                                                    </td>
                                                    <td class="remove-col">

                                                        <a onclick="deleteCart('<%= value.productId._id %>')"
                                                            class="btn btn-black btn-sm">X</a>
                                                    </td>


                                                </tr>
                                                <% }) %>
                                        </tbody>
                                    </table><!-- End .table table-wishlist -->

                                    <div class="cart-bottom">
                                        <div class="cart-discount">
                                           
                                        </div><!-- End .cart-discount -->

                                        <a href="/shop" class="btn btn-outline-dark-2">
                                            <span>UPDATE CART</span><i class="icon-refresh"></i>
                                        </a>
                                    </div>
                                </div><!-- End .col-lg-9 -->

                                <aside class="col-lg-3">
                                    <div class="summary summary-cart">
                                       
                                        <table class="table table-summary">
                                            <!-- Remaining code for the summary table -->
                                        </table><!-- End .table table-summary -->
                                        <a href="/checkout"
                                            class="btn btn-outline-primary-2 btn-order btn-block">PROCEED TO
                                            CHECKOUT</a>
                                    </div><!-- End .summary -->
                                    <a href="/shop" class="btn btn-outline-dark-2 btn-block mb-3">
                                        <span>CONTINUE SHOPPING</span><i class="icon-refresh"></i>
                                    </a>
                                </aside><!-- End .col-lg-3 -->
                            </div><!-- End .row -->
                        </div><!-- End .container -->
                        <% } else { %>
                           
                            <div class="error-content text-center" >
                                <div class="container">
                                    <h3>Your Cart is Empty</h3>
                                    <p>We are sorry, Your cart is empty</p>
                                    <a href="/shop" class="btn btn-outline-primary-2 btn-minwidth-lg">
                                        <span>BACK TO SHOP</span>
                                        <i class="icon-long-arrow-right"></i>
                                    </a>
                                </div><!-- End .container -->
                            </div><!-- End .error-content text-center -->
                            <% } %>
                </div>
                <!-- End .cart -->
            </div><!-- End .page-content -->
